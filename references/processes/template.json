{
  "_comment": "Template process profile. Copy and edit, then point config.process.path to it.",
  "id": "custom/your-process-id",
  "name": "Custom Process",
  "description": "Describe the intent and scope for this process profile.",
  "work_item_types": [
    { "type": "Epic", "slug": "epic" },
    { "type": "Feature", "slug": "feature" },
    { "type": "UserStory", "slug": "userstory" },
    { "type": "Task", "slug": "task" },
    { "type": "Bug", "slug": "bug" }
  ],
  "states": [
    "Proposed",
    "Planned",
    "Ready",
    "InProgress",
    "Review",
    "Blocked",
    "Done",
    "Dropped"
  ],
  "default_state": "Proposed",
  "terminal_states": ["Done", "Dropped"],
  "transitions": {
    "Proposed": ["Planned", "Dropped"],
    "Planned": ["Ready", "Dropped"],
    "Ready": ["InProgress", "Dropped"],
    "InProgress": ["Review", "Blocked", "Dropped"],
    "Review": ["Done", "InProgress"],
    "Blocked": ["InProgress", "Dropped"],
    "Done": [],
    "Dropped": []
  }
}
